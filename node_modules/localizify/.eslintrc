{
  "extends": "airbnb-base",
  "env": {
    "node": true
  },
  "rules": {
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,
    "class-methods-use-this": 0,
    "camelcase": 0,
    "one-var": 0,
    "comma-dangle": 0,
    "max-len": ["error", { "code": 140, "ignoreStrings": true}],
    "global-require": 0
  }
}